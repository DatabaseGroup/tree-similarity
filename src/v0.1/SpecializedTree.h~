#ifndef SPECIALIZEDTREE_H
#define SPECIALIZEDTREE_H

#include "LblTree.h"

class SpecializedTree : public LblTree {
	private:
		int dummy;

	public:
		SpecializedTree() : LblTree() { }
		SpecializedTree(int id) : LblTree(id) { }
		~SpecializedTree() { }

		const int getDummy() const;
};

#endif // SPECIALIZEDTREE_H